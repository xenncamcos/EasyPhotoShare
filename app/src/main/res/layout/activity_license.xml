<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LicenseActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="6pt"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="1dp">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4pt"
            android:autoLink="web"
            android:text="jQuery\nhttps://github.com/jquery/jquery\nCopyright JS Foundation and other contributors\nReleased under the MIT license\nhttps://opensource.org/licenses/mit-license.php"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:visibility="gone" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4pt"
            android:autoLink="web"
            android:text="simplelightbox\nhttps://github.com/andreknieriem/simplelightbox\nCopyright (C) 2016 Andre Rinas\nReleased under the MIT license\nhttps://opensource.org/licenses/mit-license.php"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:visibility="gone" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ソフトウェアのライセンス"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:autoLink="web"
            android:text='本ソフトウェアはオープンソースで提供されており拡張子"java"および"xml"のファイルに対して"Apache License, Version 2.0"が適用されています。' />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="2pt"
            android:autoLink="web"
            android:text="EasyPhotoShare\nCopyright (C) 2019 xenn\nhttps://github.com/xenncamcos/EasyPhotoShare\nReleased under the Apache License, Version 2.0\nhttp://www.apache.org/licenses/LICENSE-2.0"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="2pt"
            android:text="Playストアに関する一部のソースコードはGitHubでは公開されていません。\nまたPlayストアとGitHubへの更新は同時ではありませんのでバージョンには差が発生します。" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10pt"
            android:text="ライブラリのライセンス"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" />

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:autoLink="web"
            android:text="本ソフトウェアは以下のライブラリを利用しています。" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="4pt"
            android:autoLink="web"
            android:text="ZXing barcode scanning library\nCopyright (C) 2008 ZXing authors, Journey Mobile\nhttps://github.com/journeyapps/zxing-android-embedded\nReleased under the Apache License, Version 2.0\nhttp://www.apache.org/licenses/LICENSE-2.0"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1" />

        <TextView
            android:id="@+id/textView40"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10pt"
            android:text="サポート"
            android:textAlignment="center"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" />

        <TextView
            android:id="@+id/textView20"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:autoLink="web"
            android:text="\@xenncamcos\nhttps://twitter.com/xenncamcos\nバグ報告や機能要望はDMにてご連絡下さい。"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1" />
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
